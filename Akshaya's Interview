1.Sorting in ascending order
//array {0,1,0,1}

using System;
using System.Collections.Generic;
class Program
{
    public static void Main()
    {
        int []arr={0,1,0,1};
        Array.Sort(arr);
        foreach(var t in arr)
        {
            Console.Write(t+" ");
        }
    }
}
